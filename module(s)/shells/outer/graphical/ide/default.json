{
  "editor.accessibilityPageSize": 0,
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingComments": "never",
  "editor.autoClosingDelete": "never",
  "editor.autoClosingOvertype": "auto",
  "editor.autoClosingQuotes": "never",
  "editor.autoSurround": "never",
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.codeActions.triggerOnFocusChange": false,



  "window.menuBarVisibility": "classic",
  "remoteHub.uncommittedChangesOnEntry": "none",
  "remoteHub.commitDirectlyWarning": "off",
  "explorer.confirmDragAndDrop": false,
  "editor.fontFamily": "0xproto Nerd Font",
  "editor.detectIndentation": false,
  "settingsSync.ignoredSettings": [
    "editor.tabSize"
  ],
  "editor.tabSize": 2,

  "editor.autoIndent": "none",
  "editor.dragAndDrop": false,
  "editor.dropIntoEditor.enabled": false,
  "editor.dropIntoEditor.showDropSelector": "never",
  "editor.emptySelectionClipboard": false,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "afterDelay",
  "colorize.colorized_colors": [
    "BROWSERS_COLORS",
    "HEXA",
    "RGB",
    "HSL",
    "OKLAB",
    "OKLCH"
  ],
  "editor.fontLigatures": true
}

